generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model room{
  id          Int      @id @default(autoincrement())
  name        String   @unique
  description String
  shapes      shapes[]
  createdAt   DateTime @default(now())
}

model shapes{
  id           Int  @id @default(autoincrement())
  startOffSetX Int 
  startOffSetY Int
  endOffSetX   Int 
  endOffSetY   Int
  radius       Int
  roomId       Int
  room         room  @relation(fields:[roomId], references:[id])  
}